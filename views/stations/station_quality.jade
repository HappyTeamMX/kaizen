extends layout

block content
  .container
    h1 {{station_data.name}}
    .well.col-sm-10.col-sm-offset-1
      .table-responsive
        table.dataTable.table.table-hover
          tr(ng-repeat="order in item_list")
            td {{ order.item }}
            td
              input(type='checkbox' name='agreement')
        .col-sm-1.col-sm-offset-10
        button(class="btn btn-success", data-action="bea") Submit

